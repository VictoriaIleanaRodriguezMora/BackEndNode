<h1>Listado de productos</h1>
{{!-- {{#if productsExist}} --}}
<a href="/formProducts" >FORM</a>
    {{#each products}}
        <div>
            {{this.id}}<br/>
            {{this.title}}<br/>
            {{this.price}}<br/>
            <img src="{{this.thumbnail}}" class="product-img"/>
        </div>
    {{/each}}
{{!-- {{/if}} --}}